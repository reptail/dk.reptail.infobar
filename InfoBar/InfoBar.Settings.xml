      <Interface>a</Interface>
      <Interface>s</Interface>
      <Interface>d</Interface>
      <Interface>f</Interface>
    </NetworkMonitor>
    <!--<WinampControl>
      <Path>C:\Program Files (x86)\Winamp\winamp.exe</Path>
    </WinampControl>-->
    <!--<WinampSongTicker>
